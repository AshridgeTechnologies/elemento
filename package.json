{
  "type": "module",
  "license": "MIT",
  "scripts": {
    "compile": "tsc && cp src/appswebsite/studio/index.html build/src/studio/index.html && cp src/appswebsite/run/index.html build/src/run/index.html",
    "cleanDev": "rm -rf .parcel-cache devDist",
    "cleanBuild": "rm -rf .parcel-cache dist",
    "tscheck": "tsc --noEmit",
    "test": "jest",
    "testcoverage": "yarn test --coverage",
    "build": "NODE_OPTIONS=--no-experimental-fetch parcel build 'src/appswebsite/**/index.html'",
    "serve": "parcel --dist-dir devDist 'src/appswebsite/**/index.html' ",
    "functest": "playwright test",
    "functestprod": "PW_BASE_URL=https://elemento-apps.web.app playwright test",
    "buildRuntime": "esbuild src/runtime/index.ts --bundle --sourcemap --format=esm  --minify --outfile=dist/runtime/index.js && mkdir devDist && cp -r dist/runtime/ devDist/runtime",
    "buildServerRuntime": "esbuild src/serverRuntime/index.ts --bundle --sourcemap --format=esm  --platform=node --target=node16.17 --external:firebase-admin --minify --outfile=dist/serverRuntime/index.js && cp -r dist/serverRuntime/ devDist/serverRuntime"
  },
