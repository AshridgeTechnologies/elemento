    "lodash": "^4.17.21",
    "papaparse": "^5.3.2",
    "ramda": "^0.28.0",
    "rc-tree": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-firebaseui": "^6.0.0",
    "react-split-pane": "^0.1.92",
    "recast": "^0.21.5",
    "zen-observable": "^0.9.0",
