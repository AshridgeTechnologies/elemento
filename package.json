    "rc-tree": "^5.6.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-firebaseui": "^6.0.0",
    "react-split-pane": "^0.1.92",
    "recast": "^0.20.5",
    "zen-observable": "^0.8.15",
    "zustand": "^4.0.0-rc.1"
