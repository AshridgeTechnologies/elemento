  "scripts": {
    "compile": "yarn run tsc && cp src/editor/index.html build/src/editor/index.html && cp src/runtime/app.html build/src/runtime/app.html",
    "test": "yarn run jest",
    "testcoverage": "yarn test --coverage --coverageProvider=v8",
    "build": "yarn compile && yarn run parcel build build/src/runtime/app.html",
    "serve": "yarn compile && yarn run parcel build/src/runtime/app.html build/src/editor/index.html",
    "functest": "yarn run playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.16.1",
    "@testing-library/dom": "^8.11.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-test-renderer": "^17.0.1",
    "jest": "^27.3.1",
    "parcel": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.6",
    "react": "^17.0.2",
    "react-complex-tree": "^1.1.2",
    "react-dom": "^17.0.2"
  }
}
