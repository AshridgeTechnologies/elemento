{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/run/**",
      "destination": "/run/index.html"
    },{
      "source": "/studio/**",
      "destination": "/studio/index.html"
    }
    ]
  }
}
