{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "server_runtime_test",
  "main": "test_index.js",
  "compatibility_date": "2025-04-02",
  "observability": {
    "enabled": true
  },
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "NO_VERIFY_AUTH_TOKEN": "true"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "int-test-1",
      "database_id": "8f379425-310e-4d48-b782-1cf6137e5d29"
    }
  ],

  "durable_objects": {
    "bindings": [
      {
        "name": "TinyBaseDurableObject_Full",
        "class_name": "TinyBaseDurableObject_Full"
      },
      {
        "name": "TinyBaseDurableObject_NoAuth",
        "class_name": "TinyBaseDurableObject_NoAuth"
      },
      {
        "name": "TinyBaseDurableObject_ReadWrite",
        "class_name": "TinyBaseDurableObject_ReadWrite"
      },
      {
        "name": "TinyBaseDurableObject_A",
        "class_name": "TinyBaseDurableObject_A"
      },
      {
        "name": "TinyBaseDurableObject_B",
        "class_name": "TinyBaseDurableObject_B"
      }
    ]
  },

  "migrations": [
    {
      "tag": "v1", "new_sqlite_classes": ["TinyBaseDurableObject_A", "TinyBaseDurableObject_B", "TinyBaseDurableObject_NoAuth", "TinyBaseDurableObject_Full", "TinyBaseDurableObject_ReadWrite"]
    }
  ]
}
